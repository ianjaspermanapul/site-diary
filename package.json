{
  "name": "coding-test-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "prebuild": "expo prebuild --clean",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start --dev-client",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "db:generate": "prisma generate --schema=./app/api/db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./app/api/db/schema.prisma",
    "db:studio": "prisma studio --schema=./app/api/db/schema.prisma",
    "db:push": "prisma db push --schema=./app/api/db/schema.prisma",
    "db:seed": "tsx app/api/db/seed.ts"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@prisma/client": "^5.20.0",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.90.10",
    "dayjs": "^1.11.19",
    "expo": "^54.0.0",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "^6.0.18",
    "expo-image-picker": "^17.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "formik": "^2.4.9",
    "graphql": "^16.12.0",
    "graphql-yoga": "^5.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "prisma": "^5.20.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  },
  "prisma": {
    "seed": "tsx app/api/db/seed.ts"
  },
  "private": true
}
